<robot name="test_robot">
    <link name="base_link" > 
        <visual>
            <origin rpy="1.57 0 0" xyz="0 0 0"/>
            <geometry>
              <mesh filename="file://$(find robot_bringup)/robot_mesh/base_link.dae"/> 
            </geometry>
          </visual>
    </link >

    <link name="link1" >
      <visual>
        <origin rpy="1.57 0 1.57" xyz="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find robot_bringup)/robot_mesh/link1.dae"/> 
        </geometry>
      </visual>
    </link >

    <link name="link2" >
      <visual>
        <origin rpy="-1.57 0 1.57" xyz="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find robot_bringup)/robot_mesh/link2.dae"/> 
        </geometry>
      </visual>
    </link >

    <link name="link3" >
      <visual>
        <origin rpy="1.57 0 3.14" xyz="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find robot_bringup)/robot_mesh/link3.dae"/> 
        </geometry>
      </visual>
    </link >

    <link name="link4_artificial" >
    </link >

    <link name="link4" >
      <visual>
        <origin rpy="3.14 1.57 0" xyz="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find robot_bringup)/robot_mesh/link4.dae"/> 
        </geometry>
      </visual>
    </link >

    <link name="link4_1" >
      <visual>
        <origin rpy="3.14 1.57 0.261" xyz="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find robot_bringup)/robot_mesh/link4_1.dae"/> 
        </geometry>
      </visual>
    </link >
    
    <link name="link4_2" >
      <visual>
        <origin rpy="3.14 1.57 1.57" xyz="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find robot_bringup)/robot_mesh/link4_2.dae"/> 
        </geometry>
      </visual>
    </link >

    <link name="link4_3_artificial" >
    </link >

    <link name="link4_3" >
      <visual>
        <origin rpy="0 0 0.12" xyz="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find robot_bringup)/robot_mesh/link4_3.dae"/> 
        </geometry>
      </visual>
    </link >
  

    <!-- Joints Declaration -->
    <joint name="joint1" type="continuous">
      <parent link="base_link"/>
      <child link="link1"/>
      <axis xyz="0 0 1" />
      <origin xyz="0 0 264.5" rpy="0 0 0" />
    </joint>
  
    <joint name="joint2" type="continuous">
      <parent link="link1"/>
      <child link="link2"/>
      <axis xyz="0 0 1" />
      <origin xyz="247 168.02 332" rpy="1.57 -1.57 0" />
    </joint>
  
    <joint name="joint3" type="continuous">
      <parent link="link2"/>
      <child link="link3"/>
      <axis xyz="0 0 1" />
      <origin xyz="1100 0 0" rpy="0 0 -1.57" />
    </joint>

    <joint name="joint4_artificial" type="continuous">
      <mimic joint="joint2" multiplier="-1" offset="0" />
      <parent link="link3"/>
      <child link="link4_artificial"/>
      <axis xyz="0 0 1" />
      <origin xyz="972.83 0 102.5" rpy="0 0 0" />
    </joint>

    <joint name="joint4" type="continuous">
      <mimic joint="joint3" multiplier="-1" offset="0" />
      <parent link="link4_artificial"/>
      <child link="link4"/>
      <axis xyz="0 0 1" />
      <origin xyz="0 0 0" rpy="0 0 0" />
    </joint>

    <joint name="joint4_1" type="continuous">
      <mimic joint="joint2" multiplier="-1" offset="0" />
      <parent link="link1"/>
      <child link="link4_1"/>
      <axis xyz="0 0 1" />
      <origin xyz="-84.21 61.92 376.87" rpy="-1.57 0.02 0" />
    </joint>

    <joint name="joint4_2" type="continuous">
      <mimic joint="joint2" multiplier="1" offset="0" />
      <parent link="link4_1"/>
      <child link="link4_2"/>
      <axis xyz="0 0 1" />
      <origin xyz="10 -1100 -24.5" rpy="0 0 -0.15" />
    </joint>

    <joint name="joint4_3_artificial" type="continuous">
      <mimic joint="joint2" multiplier="-1" offset="0" />
      <parent link="link4_2"/>
      <child link="link4_3_artificial"/>
      <axis xyz="0 0 1" />
      <origin xyz="583.29 0 -70" rpy="0 0 0" />
    </joint>

    <joint name="joint4_3" type="continuous">
      <mimic joint="joint3" multiplier="-1" offset="0" />
      <parent link="link4_3_artificial"/>
      <child link="link4_3"/>
      <axis xyz="0 0 1" />
      <origin xyz="0 0 0" rpy="0 0 0" />
    </joint>


  </robot>