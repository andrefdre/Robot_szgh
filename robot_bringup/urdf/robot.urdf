<robot name="robot">
    <link name="base_link" > 
        <visual>
            <origin rpy="1.57 0 0" xyz="0 0 0"/>
            <geometry>
              <mesh filename="file://$(find robot_bringup)/robot_mesh/base_link.dae"/> 
            </geometry>
          </visual>
          <collision>
            <origin rpy="1.57 0 0" xyz="0 0 0"/>
            <geometry>
              <mesh filename="file://$(find robot_bringup)/robot_mesh/base_link.dae"/> 
            </geometry>
          </collision>
          <inertial>
            <mass value="50000"/>
            <inertia ixx="1000" ixy="0.0" ixz="0.0" iyy="1000" iyz="0.0" izz="1000"/>
          </inertial>
    </link >

    <link name="link1">
      <visual>
        <origin rpy="1.57 0 1.57" xyz="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find robot_bringup)/robot_mesh/link1.dae"/> 
        </geometry>
      </visual>
      <collision>
        <origin rpy="1.57 0 1.57" xyz="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find robot_bringup)/robot_mesh/link1.dae"/> 
        </geometry>
      </collision>
      <inertial>
        <mass value="1000"/>
        <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
      </inertial>
    </link >

    <link name="link2" >
      <visual>
        <origin rpy="-1.57 0 3.14" xyz="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find robot_bringup)/robot_mesh/link2.dae"/> 
        </geometry>
      </visual>
      <collision>
